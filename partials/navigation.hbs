{{^if isSecondary}}
<ul class="navbar-nav navbar-nav-hover align-items-lg-center ml-lg-auto">
  {{#foreach navigation from="1" to="4"}}
  <li class="nav-item">
    <a href="{{url absolute="true"}}" class="nav-link" role="button">
      <span class="nav-link-inner--text">{{label}}</span>
    </a>
  </li>
  {{/foreach}}
  <li class="nav-item dropdown {{#foreach navigation from="5" to="5"}}!{{/foreach}}is-visible">
    <a href="javascript:;" class="nav-link" data-toggle="dropdown" role="button">
      <span class="nav-link-inner--text">• • •</span>
    </a>
    <div class="dropdown-menu" aria-labelledby="navbarDropdownMenuLink">
      {{#foreach navigation from="5"}}
      <a class="dropdown-item" href="{{url absolute="true"}}">
        {{label}}
      </a>
      {{/foreach}}
    </div>
  </li>
  {{>members_login_panel}}
</ul>
{{/if}}


